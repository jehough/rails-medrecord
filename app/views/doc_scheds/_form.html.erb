
  <%= form_for @sched do |f| %>
    <%= f.label :doctor_id %>
    <%= f.collection_select :doctor_id, Doctor.all, :id, :doc_name %><br>
    <%= f.label :date %>
    <%= f.date_field :date, default: Date.today %><br>
    <%= f.label :shift_start %>
    <%= f.time_field :shift_start %><br>
    <%= f.label :shift_end %>
    <%= f.time_field :shift_end %><br>
    <%= f.submit "Create Schedule" %>
  <% end %>
